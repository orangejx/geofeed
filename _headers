# 静态资源长期缓存
/assets/*
  Cache-Control: public, max-age=31536000, immutable

# 配置文件短期缓存
/config.json
  Cache-Control: public, max-age=3600

# GeoFeed 数据短期缓存
/geofeed.csv
  Cache-Control: public, max-age=3600
  Content-Type: text/csv; charset=utf-8

# HTML 页面短期缓存
/index.html
  Cache-Control: public, max-age=3600
